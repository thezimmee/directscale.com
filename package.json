    "@bundles/core": "^0.9.3",
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "autoprefixer": "^9.5.1",
    "browser-sync": "^2.26.7",
    "cheerio": "^1.0.0-rc.3",
    "conventional-changelog-cli": "^2.0.21",
    "cssnano": "^4.1.10",
    "cssstats": "^3.3.0",
    "get-css-colors": "^3.3.0",
    "gh-pages": "^2.1.1",
    "highlight.js": "^9.15.6",
    "html-minifier": "^4.0.0",
    "loglevel": "^1.6.1",
    "lorem-ipsum": "^2.0.2",
    "markdown-it": "^8.4.2",
    "minireset.css": "0.0.5",
    "normalize.css": "^8.0.1",
    "postcss": "^7.0.16",
    "postcss-calc": "^7.0.1",
    "postcss-css-variables": "^0.12.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-functions": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.1",
    "postcss-nested": "^4.1.2",
    "postcss-nested-ancestors": "^2.0.0",
    "postcss-pxtorem": "^4.0.1",
    "postcss-reporter": "^6.0.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.0.0",
    "size-limit": "^1.3.2",
    "svgo": "^1.3.0"
  },
  "scripts": {
    "start": "npm run watch",
    "size": "size-limit",
    "prebuild": "rimraf build",
    "build": "bundle --config=.bundlesrc.js && rollup --config=.rolluprc.js",
    "prod": "NODE_ENV=production npm run build && npm run serve",
    "prewatch": "rimraf build",
    "watch": "bundle --config=.bundlesrc.js --watch & rollup --config=.rolluprc.js --watch",
    "serve": "browser-sync start --config=.browsersyncrc.js",
    "predeploy": "NODE_ENV=production npm run build && echo \"Publishing to github pages...\"",
    "deploy": "gh-pages -d ./build --repo https://thezimmee@github.com/thezimmee/directscale.com.git"
  },
  "main": "build/app.js",
  "keywords": [],
  "author": "DirectScale",
  "homepage": "https://directscale.com",
  "contributors": ["TZimmee <thezimmee@gmail.com> (https://tysonzimmerman.com)"]
}
